# Docker environment variables for flyfunboarding-api
# Copy to .env and update with your values

# Database credentials (must match what's configured in shared-infra MySQL)
DB_USER=flyfunboarding
DB_PASSWORD=your_secure_password
DB_NAME=flyfunboarding

# Security
SECRET=your-secret-key-here
USE_PUBLIC_KEY_SIGNATURE=true

# PKPass certificate password (if any)
CERTIFICATE_PASSWORD=

# API Config
DEBUG=false
LOG_LEVEL=INFO
CORS_ORIGINS=*
